{
  "name": "angular2-minimum",
  "version": "1.0.0",
  "description": "要件を満たすAngular2の最小構成を探る。他のプロジェクトのテンプレートとなることを目指す。",
  "main": "index.html",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "lite": "lite-server -c bs-config.json",
    "start": "npm run webpack && npm run lite",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "start:aot": "npm run ngc && npm start",
    "test": "karma start --single-run"
  },
  "reository": {
    "type": "git",
    "url": "git+https://github.com/KadoTomohiro/angular2-minimum"
  },
  "keywords": [
    "angular2",
    "AoT",
    "webpack"
  ],
  "author": "KadoTomohiro",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/platform-server": "2.0.2",
    "@angular/router": "3.0.0",
    "@angular/compiler-cli": "^0.6.4",
    "@types/jasmine": "^2.5.35",
    "@types/karma": "^0.13.33",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "typescript": "^2.0.3",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/es6-shim": "0.0.31",
    "@types/node": "^6.0.38",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "lite-server": "^2.2.2",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "2.0.3",
    "webpack": "2.1.0-beta.22"
  }
}
